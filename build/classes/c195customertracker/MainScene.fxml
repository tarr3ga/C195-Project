<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="800.0" prefWidth="1200.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="c195customertracker.MainSceneController">
     <stylesheets>
        <String fx:value="styles/stylesMain.css" />
    </stylesheets>
    <children>
            <AnchorPane fx:id="navigation">
               <children>
                   <Button fx:id="btnHome" layoutX="10" minWidth="160.0" prefWidth="160.0" styleClass="mainFxmlClass" 
                           text="Home" onAction="#onHomeClick" />
                   <Button fx:id="btnCustomers" layoutX="180" minWidth="160.0" prefWidth="160.0" styleClass="mainFxmlClass" 
                           text="Customers" onAction="#onCustomersClick" />
                   <Button fx:id="btnAppointment" layoutX="350.0" minWidth="160.0" prefWidth="160.0" 
                           text="Appointments" onAction="#onAppointmentsClick" />
                   <Label fx:id="pageTitle" id="pageTitle" layoutX="600.0" minHeight="45" minWidth="200" maxWidth="200" />
               </children>
            </AnchorPane> 

            <GridPane fx:id="display" layoutY="70">
                
            </GridPane>
    </children>
</AnchorPane>
